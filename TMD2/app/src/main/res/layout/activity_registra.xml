<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".registra">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="2dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="16dp"
                app:layout_constraintEnd_toStartOf="@+id/textView2"
                app:layout_constraintHorizontal_bias="0.528"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/logo" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="&quot;DEKE Android&quot;"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.493"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintVertical_bias="1.0" />


            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <EditText
                android:id="@+id/nuSeguroRegistro"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="16dp"
                android:backgroundTintMode="add"
                android:ems="10"
                android:hint="Numero de Seguro Social"
                android:inputType="number"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/nuClinicaRegistro"
                tools:layout_editor_absoluteX="16dp" />

            <EditText
                android:id="@+id/nuClinicaRegistro"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="24dp"
                android:ems="10"
                android:backgroundTintMode="add"
                android:hint="Numero de tu clinica"
                android:inputType="number"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/nombreRegistro"
                tools:layout_editor_absoluteX="16dp" />

            <EditText
                android:id="@+id/nombreRegistro"
                android:backgroundTintMode="add"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="28dp"
                android:ems="10"
                android:hint="Nombre"
                android:inputType="textPersonName"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/apellidoRegistro"
                tools:layout_editor_absoluteX="16dp" />

            <EditText
                android:id="@+id/apellidoRegistro"
                android:backgroundTintMode="add"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="28dp"
                android:ems="10"
                android:hint="Apellido"
                android:inputType="textPersonName"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/fechaNacimientoRegistro"
                tools:layout_editor_absoluteX="16dp" />

            <EditText
                android:id="@+id/fechaNacimientoRegistro"
                android:backgroundTintMode="add"
                android:layout_width="305dp"
                android:layout_height="48dp"
                android:layout_marginBottom="24dp"
                android:ems="10"
                android:hint="Fecha de Nacimiento"
                android:inputType="textPersonName"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/dirrecionRegistro"
                tools:layout_editor_absoluteX="16dp" />

            <EditText
                android:id="@+id/dirrecionRegistro"
                android:layout_width="308dp"
                android:backgroundTintMode="add"
                android:layout_height="44dp"
                android:layout_marginBottom="24dp"
                android:ems="10"
                android:hint="Dirrecion"
                android:inputType="textPersonName"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/cpRegistro"
                tools:layout_editor_absoluteX="16dp" />

            <EditText
                android:id="@+id/cpRegistro"
                android:layout_width="199dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:backgroundTintMode="add"
                android:ems="10"
                android:hint="CP"
                android:inputType="number"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/nuTelefonoRegistro"
                tools:layout_editor_absoluteX="16dp" />

            <EditText
                android:id="@+id/nuTelefonoRegistro"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:ems="10"
                android:hint="Numero de Telefono"
                android:inputType="number"
                android:textSize="20sp"
                android:backgroundTintMode="add"
                tools:layout_editor_absoluteX="16dp" />

            <EditText
                android:id="@+id/correo"
                android:layout_width="332dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:backgroundTintMode="add"
                android:ems="10"
                android:hint="Correo electronico"
                android:inputType="textEmailAddress"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="16dp" />

            <EditText
                android:id="@+id/contrasena"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:ems="10"
                android:backgroundTintMode="add"
                android:hint="ContraseÃ±a"
                android:inputType="textPersonName"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/registrar"
                tools:layout_editor_absoluteX="16dp" />


            <CheckBox
                android:id="@+id/TYC"
                android:layout_width="match_parent"
                android:layout_height="59dp"
                android:text="Aceptar terminos y condiciones"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/error"
                android:layout_width="match_parent"
                android:layout_height="21dp"
                android:textSize="16sp" />

            <Button
                android:id="@+id/registrar"
                android:layout_width="213dp"
                android:layout_height="wrap_content"
                android:background="@drawable/circular"
                android:text="Registrar"
                android:layout_marginLeft="10dp"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                tools:layout_editor_absoluteY="678dp" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>